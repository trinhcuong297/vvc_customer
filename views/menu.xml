    <!-- qr code -->
    <record id="vvc_qr_action_form" model="ir.ui.view">
        <field name="name">customer.qr.form</field>
        <field name="model">customer.qr</field>
        <field name="arch" type="xml">
            <form string="QR Code">
                <header>
                    <field name="status" widget="statusbar" nolabel="1"></field>
                </header>
                <sheet>
                    <group>
                        <field name="timeScan"/>
                        <field name="qrCodeScan" widget="qr_code_widget"/>
                        <field name="type"/>
                        <field name="ground_ids"/>
                        <field name="block"/>
                        <field name="building"/>
                        <field name="water" attrs="{'invisible': [('type', '=', 'elec')]}"/>
                        <field name="elec" attrs="{'invisible': [('type', '=', 'water')]}"/>
                        <field name="recordImage"  widget="image"/>
                    </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="activity_ids" widget="mail_activity"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
        </field>
    </record>

    <record id="vvc_qr_action_tree" model="ir.ui.view">
        <field name="name">customer.qr.tree</field>
        <field name="model">customer.qr</field>
        <field name="arch" type="xml">
            <tree string="List QR">
                <field name="timeScan"/>
                <field name="type"/>
                <field name="qrCodeScan"/>
                <field name="water" attrs="{'invisible': [('type', '=', 'elec')]}"/>
                <field name="elec" attrs="{'invisible': [('type', '=', 'water')]}"/>
                <field name="ground_ids"/>
                <field name="block"/>
            </tree>
        </field>
    </record>

    <record id="vvc_qr_action_search" model="ir.ui.view">
