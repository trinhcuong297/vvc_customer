<?xml version="1.0" encoding="utf-8" ?>
<odoo>
   
    <record id="vvc_customer_action_form" model="ir.ui.view">
        <field name="name">customer.account.form</field>
        <field name="model">customer.account</field>
        <field name="arch" type="xml">
            <form string="Chi tiết khách hàng">
                <sheet>
                    <group>
                        <field name="name"></field>
                        <field name="birthday"></field>
                        <field name="phone"></field>
                        <field name="email"></field>
                        <field name="gender"></field>
                        <field name="IDCard"></field>
                        <field name="studyLevel"></field>
                        <field name="job"></field>
                        <field name="elevatorCard"></field>
                        <field name="isCompany"></field>
                        <field name="password"></field>
                        <field name="ground_ids"></field>
                    </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="activity_ids" widget="mail_activity"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
        </field>
    </record>

    <record id="vvc_customer_action_tree" model="ir.ui.view">
        <field name="name">customer.account.tree</field>
        <field name="model">customer.account</field>
        <field name="arch" type="xml">
            <tree string="List khách hàng">
                <field name="id"></field>
                <field name="name"></field>
                <field name="phone"></field>
                <field name="building"></field>
                <field name="block"></field>
                <field name="ground_ids" widget="many2many_tags"></field>
                <field name="archiveCount"></field>
                <field name="billCount"></field>
                <field name="errorCount"></field>
            </tree>
        </field>
    </record>

    <record id="vvc_customer_action_search" model="ir.ui.view">
        <field name="name">customer.account.search</field>
        <field name="model">customer.account</field>
        <field name="arch" type="xml">
            <search>
                <field name="name"></field>
                <field name="id"></field>
                <field name="isCompany"></field>
                <field name="ground_ids"></field>
            </search>
        </field>
    </record>

    <record id="vvc_customer_action" model="ir.actions.act_window">
        <field name="type">ir.actions.act_window</field>
        <field name="name">Khách hàng</field>
        <field name="res_model">customer.account</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
    </record>

    <!-- archive -->

    <record id="vvc_archive_action_form" model="ir.ui.view">
        <field name="name">customer.archive.form</field>
        <field name="model">customer.archive</field>
        <field name="arch" type="xml">
            <form string="Chi tiết đơn hàng">
                <sheet>
                    <group>
                        <group>
                            <field name="user_id"/>
                            <field name="name"/>
                            <field name="phone"/>
                            <field name="ground_ids"/>
                            <field name="block"/>
                            <field name="building"/>
                            <field name="inputTime"/>
                            <field name="outputTime"/>
                            <field name="status" />
                            <field name="desc" />
                        </group>
                        <group>
                            <field name="inputImage" widget="image"/>
                            <field name="outputImage" widget="image"/>
                        </group>
                    </group>
                </sheet>
                <div class="oe_chatter">
                    <field name="message_follower_ids" widget="mail_followers"/>
                    <field name="activity_ids" widget="mail_activity"/>
                    <field name="message_ids" widget="mail_thread"/>
                </div>
            </form>
        </field>
    </record>

    <record id="vvc_archive_action_tree" model="ir.ui.view">
        <field name="name">customer.archive.tree</field>
        <field name="model">customer.archive</field>
        <field name="arch" type="xml">
            <tree string="List đơn hàng" 
            decoration-success="status == 'sended'"
            decoration-info="status == 'store'"
            >
                <field name="id"/>
                <field name="user_id"/>
                <field name="name"/>
                <field name="phone"/>
                <field name="inputTime"/>
                <field name="outputTime"/>
                <field name="ground_ids" widget="many2many_tags"/>
                <field name="block"/>
                <field name="building"/>
                <field name="status" />
            </tree>
        </field>
    </record>

    <record id="vvc_archive_action_search" model="ir.ui.view">
        <field name="name">customer.archive.search</field>
        <field name="model">customer.archive</field>
        <field name="arch" type="xml">
            <search>
                <field name="id"/>
                <field name="status" />
                <field name="user_id"/>
                <field name="name"/>
                <field name="phone"/>
                <field name="ground_ids"/>
                <field name="block"/>
                <field name="inputTime"/>
                <field name="outputTime"/>
                <field name="building"/>
                <field name="inputImage"/>
                <field name="outputImage"/>
            </search>
        </field>
    </record>

    <record id="vvc_archive_action" model="ir.actions.act_window">
        <field name="type">ir.actions.act_window</field>
        <field name="name">Hàng kí gửi</field>
        <field name="res_model">customer.archive</field>
        <field name="view_mode">tree,form</field>
        <field name="context">{}</field>
